{
  "name": "alfabeta-price-updater",
  "version": "1.0.0",
  "description": "Actualizador de precios de Alfabeta",
  "main": "dist/src/server.js",
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "predev": "sequelize db:migrate",
    "dev": "nodemon --watch 'src/**' --ext 'ts,json' --ignore 'src/**/*.spec.ts' --exec 'ts-node -r tsconfig-paths/register ./src/server.ts' --ignore 'node_modules'",
    "migrate": "sequelize db:migrate",
    "seeder": "sequelize db:seed:all",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "prestart": "npm run migrate && npm run seeder",
    "start": "node .",
    "prod": "npm run build && npm run start",
    "pretest": "sequelize db:migrate",
    "test": "jest",
    "posttest": "sequelize db:migrate:undo:all",
    "test:watch": "jest --watch --coverage=false",
    "test:coverage": "jest --watchAll --coverage --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinrast/alfabeta-price-updater.git"
  },
  "keywords": [],
  "author": "Vincen Santaella",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vinrast/alfabeta-price-updater/issues"
  },
  "homepage": "https://github.com/vinrast/alfabeta-price-updater#readme",
  "devDependencies": {
    "@keenondrums/singleton": "1.0.0",
    "@types/axios": "0.14.0",
    "@types/express": "4.17.9",
    "@types/factory-girl": "5.0.2",
    "@types/faker": "5.1.5",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "@types/nodemailer": "^6.4.0",
    "@types/sequelize": "4.28.9",
    "@types/superagent": "4.1.10",
    "@types/supertest": "^2.0.11",
    "@types/validator": "13.1.0",
    "@types/validatorjs": "3.15.0",
    "factory-girl": "5.0.4",
    "faker": "5.1.0",
    "nodemon": "^2.0.20",
    "sequelize-cli": "6.2.0",
    "supertest": "4.0.2",
    "ts-jest": "29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "3.9.0",
    "tslib": "^2.5.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/xml-js": "^1.0.0",
    "@types/xml2js": "^0.4.11",
    "adm-zip": "^0.5.10",
    "axios": "0.21.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "jest": "^29.4.3",
    "pg": "8.5.1",
    "pg-hstore": "2.3.3",
    "sequelize": "6.3.5",
    "soap": "^1.0.0",
    "uuidv4": "6.2.6",
    "validatorjs": "3.22.1",
    "xml2js": "^0.4.23",
    "nodemailer": "^6.6.1"
  }
}
